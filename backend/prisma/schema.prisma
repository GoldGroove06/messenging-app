datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


generator client {
  provider = "prisma-client-js"
}

model Session {
  id        String   @id
  sid       String   @unique
  data     String
  expiresAt   DateTime
}

model User {
  id       Int      @id @default(autoincrement())
  name     String
  email    String   @unique
  password String
  username String   @unique  
}
